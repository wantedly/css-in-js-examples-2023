{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react-jsx",
    "useDefineForClassFields": true,

    "module": "nodenext",

    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
